[color]
	branch = auto
	diff = auto
	interactive = auto
	status = auto
[core]
	excludesfile = ~/.gitignore_global
[alias]
	lg = log --color --graph --pretty=format:'%C(dim normal)%h%Creset%C(yellow)%d%Creset %s %C(dim normal)%cr %an%Creset' --abbrev-commit --
	wtf = !git-wtf
	ci = commit
	st = status
